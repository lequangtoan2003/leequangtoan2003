<!DOCTYPE html>
<html>
  <head>
    <title>Khách hàng</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>Danh sách khách hàng</h1>
    <a href="/form-add"><button>Thêm khách hàng</button></a> <br />
    <table border="1">
      <tr>
        <th>ID</th>
        <th>Id District</th>
        <th>Id Village</th>
        <th>Address</th>
        <th>Bank Name</th>
        <th>Branch Name</th>
        <th>ACTION</th>
      </tr>
      <% custommer4s.forEach(item => { %>
      <tr>
        <td><%= item._id%></td>
        <td><%= item.idDistrict%></td>
        <td><%= item.idVillage%></td>
        <td><%= item.address%></td>
        <td><%= item.bankName%></td>
        <td><%= item.branchName%></td>
        <td>
          <a href="/form-update/<%= item._id%>">Update</a>||<a href="/form-delete/<%= item._id%>">Delete</a
          >
        </td>
      </tr>
      <% }) %>
    </table>
  </body>
</html>
